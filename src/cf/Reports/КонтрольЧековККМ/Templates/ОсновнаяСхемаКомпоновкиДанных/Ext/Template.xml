		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ОтчетОРозничныхПродажах.Склад,
	ОтчетОРозничныхПродажах.Ссылка КАК ОРП,
	НАЧАЛОПЕРИОДА(ОтчетОРозничныхПродажах.Дата, ДЕНЬ) КАК Период,
	ОтчетОРозничныхПродажах.ZОтчетНомер КАК НомерСмены,
	ОтчетОРозничныхПродажах.ZСуммаКон - ОтчетОРозничныхПродажах.ZСуммаНач КАК СуммаZОтчетаОРП,
	ОтчетОРозничныхПродажах.СуммаДокумента КАК СуммаДокументаОРП
ПОМЕСТИТЬ ВР_ОРП_БезРазделенияПоВидамОплаты
ИЗ
	Документ.ОтчетОРозничныхПродажах КАК ОтчетОРозничныхПродажах
ГДЕ
	ОтчетОРозничныхПродажах.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И НЕ ОтчетОРозничныхПродажах.ПометкаУдаления
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.Ссылка,
	ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.ВидОплаты,
	СУММА(ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.Сумма) КАК Сумма
ПОМЕСТИТЬ ВР_ОРП_ОплатаКартами
ИЗ
	Документ.ОтчетОРозничныхПродажах.ОплатаПлатежнымиКартами КАК ОтчетОРозничныхПродажахОплатаПлатежнымиКартами
ГДЕ
	ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.Ссылка В
			(ВЫБРАТЬ
				ВР_ОРП_БезРазделенияПоВидамОплаты.ОРП
			ИЗ
				ВР_ОРП_БезРазделенияПоВидамОплаты КАК ВР_ОРП_БезРазделенияПоВидамОплаты)

СГРУППИРОВАТЬ ПО
	ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.Ссылка,
	ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.ВидОплаты

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ДляГруппировкиНаличногоВидаОплаты.Ссылка,
	ДляГруппировкиНаличногоВидаОплаты.ВидОплаты,
	СУММА(ДляГруппировкиНаличногоВидаОплаты.Сумма)
ИЗ
	(ВЫБРАТЬ
		ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.Ссылка КАК Ссылка,
		ЗНАЧЕНИЕ(Справочник.ВидыОплатЧекаККМ.Наличные) КАК ВидОплаты,
		ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.Сумма КАК Сумма
	ИЗ
		Документ.ОтчетОРозничныхПродажах.ОплатаПлатежнымиКартами КАК ОтчетОРозничныхПродажахОплатаПлатежнымиКартами
	ГДЕ
		ОтчетОРозничныхПродажахОплатаПлатежнымиКартами.Ссылка В
				(ВЫБРАТЬ
					ВР_ОРП_БезРазделенияПоВидамОплаты.ОРП
				ИЗ
					ВР_ОРП_БезРазделенияПоВидамОплаты КАК ВР_ОРП_БезРазделенияПоВидамОплаты)) КАК ДляГруппировкиНаличногоВидаОплаты

СГРУППИРОВАТЬ ПО
	ДляГруппировкиНаличногоВидаОплаты.Ссылка,
	ДляГруппировкиНаличногоВидаОплаты.ВидОплаты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВР_ОРП_БезРазделенияПоВидамОплаты.Склад,
	ВР_ОРП_БезРазделенияПоВидамОплаты.ОРП,
	ВР_ОРП_БезРазделенияПоВидамОплаты.Период,
	ВР_ОРП_БезРазделенияПоВидамОплаты.НомерСмены,
	ВР_ОРП_БезРазделенияПоВидамОплаты.СуммаZОтчетаОРП,
	ВР_ОРП_БезРазделенияПоВидамОплаты.СуммаДокументаОРП,
	ЕСТЬNULL(ВР_ОРП_ОплатаКартами.ВидОплаты, ЗНАЧЕНИЕ(Справочник.ВидыОплатЧекаККМ.Наличные)) КАК ВидОплаты,
	ВЫБОР
		КОГДА ВР_ОРП_ОплатаКартами.ВидОплаты = &amp;Наличные ИЛИ ВР_ОРП_ОплатаКартами.ВидОплаты Есть NULL
			ТОГДА ВР_ОРП_БезРазделенияПоВидамОплаты.СуммаДокументаОРП - ЕСТЬNULL(ВР_ОРП_ОплатаКартами.Сумма, 0)
		ИНАЧЕ ВР_ОРП_ОплатаКартами.Сумма
	КОНЕЦ КАК СуммаОРП
ПОМЕСТИТЬ ВР_ОРП
ИЗ
	ВР_ОРП_БезРазделенияПоВидамОплаты КАК ВР_ОРП_БезРазделенияПоВидамОплаты
		ЛЕВОЕ СОЕДИНЕНИЕ ВР_ОРП_ОплатаКартами КАК ВР_ОРП_ОплатаКартами
		ПО ВР_ОРП_БезРазделенияПоВидамОплаты.ОРП = ВР_ОРП_ОплатаКартами.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЧекиККМ.Документ,
	НАЧАЛОПЕРИОДА(ЧекиККМ.Период, ДЕНЬ) КАК Период,
	ЧекиККМ.ВидОплатыЧекаККМ,
	ЧекиККМ.Склад,
	ЧекиККМ.ВидыОперацийЧекККМ,
	ЧекиККМ.Пользователь,
	ЧекиККМ.НомерЧека,
	СУММА(ВЫБОР
			КОГДА ЧекиККМ.ВидыОперацийЧекККМ = &amp;Продажа
				ТОГДА ЧекиККМ.Сумма
			ИНАЧЕ -1 * ЧекиККМ.Сумма
		КОНЕЦ) КАК СуммаЧека,
	ЧекиККМ.НомерСмены
ПОМЕСТИТЬ Чеки
ИЗ
	РегистрСведений.ЧекиККМ КАК ЧекиККМ
ГДЕ
	ЧекиККМ.Период МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания

СГРУППИРОВАТЬ ПО
	ЧекиККМ.ВидОплатыЧекаККМ,
	НАЧАЛОПЕРИОДА(ЧекиККМ.Период, ДЕНЬ),
	ЧекиККМ.Документ,
	ЧекиККМ.Пользователь,
	ЧекиККМ.Склад,
	ЧекиККМ.ВидыОперацийЧекККМ,
	ЧекиККМ.НомерЧека,
	ЧекиККМ.НомерСмены
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НАЧАЛОПЕРИОДА(РеализацияТоваровУслуг.Дата, ДЕНЬ) КАК Период,
	РеализацияТоваровУслуг.Ссылка КАК Документ,
	РеализацияТоваровУслуг.Склад,
	РеализацияТоваровУслуг.ВидОплаты,
	СУММА(РеализацияТоваровУслуг.СуммаДокумента) КАК СуммаДокумента
ПОМЕСТИТЬ Документы1С
ИЗ
	Документ.РеализацияТоваровУслуг КАК РеализацияТоваровУслуг
ГДЕ
	РеализацияТоваровУслуг.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И НЕ РеализацияТоваровУслуг.ПометкаУдаления

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(РеализацияТоваровУслуг.Дата, ДЕНЬ),
	РеализацияТоваровУслуг.ВидОплаты,
	РеализацияТоваровУслуг.Ссылка,
	РеализацияТоваровУслуг.Склад

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	НАЧАЛОПЕРИОДА(ВозвратТоваровОтПокупателя.Сделка.Дата, ДЕНЬ),
	ВозвратТоваровОтПокупателя.Ссылка,
	ВозвратТоваровОтПокупателя.СкладОрдер,
	ВозвратТоваровОтПокупателя.Сделка.ВидОплаты,
	СУММА(-1 * ВозвратТоваровОтПокупателя.СуммаДокумента)
ИЗ
	Документ.ВозвратТоваровОтПокупателя КАК ВозвратТоваровОтПокупателя
ГДЕ
	ВозвратТоваровОтПокупателя.Сделка.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ВозвратТоваровОтПокупателя.Сделка ССЫЛКА Документ.РеализацияТоваровУслуг
	И ВозвратТоваровОтПокупателя.Сделка &lt;&gt; ЗНАЧЕНИЕ(Документ.РеализацияТоваровУслуг.ПустаяСсылка)
	И НЕ ВозвратТоваровОтПокупателя.ПометкаУдаления

СГРУППИРОВАТЬ ПО
	ВозвратТоваровОтПокупателя.Ссылка,
	НАЧАЛОПЕРИОДА(ВозвратТоваровОтПокупателя.Сделка.Дата, ДЕНЬ),
	ВозвратТоваровОтПокупателя.СкладОрдер,
	ВозвратТоваровОтПокупателя.Сделка.ВидОплаты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(Чеки.Документ, Документы1С.Документ) КАК Документ,
	ЕСТЬNULL(Чеки.Период, Документы1С.Период) КАК Период,
	ЕСТЬNULL(Чеки.ВидОплатыЧекаККМ, Документы1С.ВидОплаты) КАК Нал_БезНал,
	ЕСТЬNULL(Чеки.Склад, Документы1С.Склад) КАК Склад,
	Чеки.ВидыОперацийЧекККМ КАК Продажа_Возврат,
	Чеки.Пользователь,
	Чеки.НомерЧека,
	СУММА(Чеки.СуммаЧека) КАК СуммаЧека,
	Чеки.НомерСмены,
	СУММА(Документы1С.СуммаДокумента) КАК СуммаДокумента
ПОМЕСТИТЬ ВР_Чеки
ИЗ
	Документы1С КАК Документы1С
		ПОЛНОЕ СОЕДИНЕНИЕ Чеки КАК Чеки
		ПО Документы1С.Документ = Чеки.Документ
			И Документы1С.Период = Чеки.Период
			И Документы1С.ВидОплаты = Чеки.ВидОплатыЧекаККМ
			И Документы1С.Склад = Чеки.Склад

СГРУППИРОВАТЬ ПО
	Чеки.ВидыОперацийЧекККМ,
	Чеки.Пользователь,
	ЕСТЬNULL(Чеки.Период, Документы1С.Период),
	ЕСТЬNULL(Чеки.Документ, Документы1С.Документ),
	ЕСТЬNULL(Чеки.Склад, Документы1С.Склад),
	ЕСТЬNULL(Чеки.ВидОплатыЧекаККМ, Документы1С.ВидОплаты),
	Чеки.НомерЧека,
	Чеки.НомерСмены
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ВР_ОРП.Склад, ВР_Чеки.Склад) КАК Склад,
	ЕСТЬNULL(ВР_ОРП.Период, ВР_Чеки.Период) КАК Период,
	ЕСТЬNULL(ВР_ОРП.НомерСмены, ВР_Чеки.НомерСмены) КАК НомерСмены,
	ВР_ОРП.ОРП,
	СУММА(ЕСТЬNULL(ВР_ОРП.СуммаZОтчетаОРП, 0)) КАК СуммаZОтчетаОРП,
	СУММА(ЕСТЬNULL(ВР_ОРП.СуммаДокументаОРП, 0)) КАК СуммаДокументаОРП,
	ЕСТЬNULL(ВР_Чеки.Нал_БезНал, ВР_ОРП.ВидОплаты) КАК Нал_БезНал,
	ВР_Чеки.Продажа_Возврат,
	ВР_Чеки.Пользователь,
	ВР_Чеки.НомерЧека,
	СУММА(ЕСТЬNULL(ВР_Чеки.СуммаЧека, 0)) КАК СуммаЧека,
	СУММА(ЕСТЬNULL(ВР_Чеки.СуммаДокумента, 0)) КАК СуммаДокумента,
	ВР_Чеки.Документ,
	СУММА(ЕСТЬNULL(ВР_ОРП.СуммаОРП, 0)) КАК СуммаОРП
ИЗ
	ВР_ОРП КАК ВР_ОРП
		ПОЛНОЕ СОЕДИНЕНИЕ ВР_Чеки КАК ВР_Чеки
		ПО ВР_ОРП.Склад = ВР_Чеки.Склад
			И ВР_ОРП.Период = ВР_Чеки.Период
			И ВР_ОРП.НомерСмены = ВР_Чеки.НомерСмены
			И ВР_ОРП.ВидОплаты = ВР_Чеки.Нал_БезНал

СГРУППИРОВАТЬ ПО
	ЕСТЬNULL(ВР_ОРП.Склад, ВР_Чеки.Склад),
	ЕСТЬNULL(ВР_ОРП.Период, ВР_Чеки.Период),
	ЕСТЬNULL(ВР_ОРП.НомерСмены, ВР_Чеки.НомерСмены),
	ЕСТЬNULL(ВР_Чеки.Нал_БезНал, ВР_ОРП.ВидОплаты),
	ВР_ОРП.ОРП,
	ВР_Чеки.Продажа_Возврат,
	ВР_Чеки.НомерЧека,
	ВР_Чеки.Пользователь,
	ВР_Чеки.Документ</query>
	</dataSet>
	<totalField>
		<dataPath>СуммаДокумента</dataPath>
