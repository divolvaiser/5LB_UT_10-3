		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ВЫБОР
		КОГДА Рег.ШтрихКод = ""
			ТОГДА Рег.Номер
		ИНАЧЕ 0
	КОНЕЦ КАК Номер,
	Рег.ШтрихКод,
	Рег.МестоПродажи,
	Рег.Номинал,
	Рег.МестоПогашения,
	ДокРеал.Ссылка.Дата КАК ДатаРеализации,
	ДокРеал.Ссылка.Номер КАК НомерРеализации,
	ДокГашение.Ссылка.Дата КАК ДатаГашения,
	ДокГашение.Ссылка.Номер КАК НомерГашения
ИЗ
	РегистрСведений.ПодарочныеКарты КАК Рег
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.РеализацияТоваровУслуг.ПодарочныеКарты КАК ДокРеал
		ПО Рег.Номер = ДокРеал.Номер
			И (ДокРеал.Ссылка.Проведен)
			И (ДокРеал.Сумма = 0)
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.РеализацияТоваровУслуг.ПодарочныеКарты КАК ДокГашение
		ПО Рег.Номер = ДокГашение.Номер
			И (ДокГашение.Ссылка.Проведен)
			И (ДокГашение.Сумма &lt;&gt; 0)
{ГДЕ
	(Рег.ДатаПродажи МЕЖДУ &amp;Дат1 И &amp;Дат2)}</query>
